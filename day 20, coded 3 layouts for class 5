(((layout 3)))

html-----

<!DOCTYPE html>
<html>
    <!-- what the computer sees -->
    <head>
        <title>Homework</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="cl4.3.css" rel="stylesheet">
    </head>

    <body>
        <header></header>

        <section>
            <div class="one"></div>
            <div class="two"></div>
            <div class="three"></div>
            <div class="four"></div>
        </section>
        
        <footer></footer>
    </body>
</html>    

css------

body {
    width: 500px;
    height: 300px;
    border: 1px solid black;
}

header, footer {
    width: 500px;
    height: 50px;
}

section {
    width: 500px;
    height: 200px;
}

.one {
    float: left;
    height: 200px;
    width: 89px;
    border: 1px solid black;
}

.two {
    float: left;
    height: 200px;
    width: 225px;
    border: 1px solid black;
}

.three {
    float: left;
    height: 200px;
    width: 89px;
    border: 1px solid black;
}

.four {
    float: left;
    height: 200px;
    width: 89px;
